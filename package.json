{
	"name": "sveltycms",
	"description": "A powerful Headless CMS with sveltekit power",
	"version": "0.0.5",
	"scripts": {
		"---- INSTALLER ----------------------------------------------------------": "",
		"installer": "node cli-installer/cli-installer.js",
		"---- DEVELOP ----------------------------------------------------------": "",
		"dev": "vite dev",
		"devv": "LAUNCH_EDITOR='code' vite --open",
		"---- BUILD ----------------------------------------------------------": "",
		"build": "paraglide-js compile --project ./project.inlang && vite build",
		"---- CHECK & TEST ----------------------------------------------------------": "",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json  --threshold error",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "paraglide-js compile && playwright test",
		"test:e2e": "pnpm dev & pnpx wait-on http://localhost:5173 && playwright test",
		"---- LINT & FORMAT ------------------------------------------------------------": "",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"---- OTHER ----------------------------------------------------------": "",
		"preview": "vite preview"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Rar9/SveltyCMS"
	},
	"bugs": {
